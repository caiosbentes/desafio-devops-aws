apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: comment-api
spec:
  rules:
  - host: comment-api.com.br
    http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name:comment-api
            port:
              number: 5000
  ingressClassName: nginx
